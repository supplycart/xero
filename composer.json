{
  "name": "supplycart/xero",
  "description": "Xero integration",
  "scripts": {
    "test": "vendor/bin/phpunit",
    "fix": "vendor/bin/php-cs-fixer"
  },
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0.1",
    "ext-json": "*",
    "phpunit/phpunit": "^9.5",
    "spatie/data-transfer-object": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "6.24.1",
    "friendsofphp/php-cs-fixer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Supplycart\\Xero\\": "src/"
    },
    "classmap": [
      "database/migrations"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Supplycart\\Xero\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Supplycart\\Xero\\XeroServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Azri Ismail",
      "email": "mazri.iar@gmail.com"
    }
  ]
}
